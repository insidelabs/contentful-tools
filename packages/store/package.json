{
    "name": "@contentful-tools/store",
    "version": "0.7.3",
    "description": "An auto-synchronizing in-memory store for Contentful content",
    "keywords": [
        "contentful"
    ],
    "author": "Inside Labs",
    "homepage": "https://github.com/insidelabs/contentful-tools/tree/master/packages/store#readme",
    "license": "ISC",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "directories": {
        "lib": "lib",
        "test": "lib/__tests__"
    },
    "files": [
        "/lib/types/",
        "/lib/*.*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/insidelabs/contentful-tools.git",
        "directory": "packages/store"
    },
    "bugs": {
        "url": "https://github.com/insidelabs/contentful-tools/issues"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "dependencies": {
        "debug": "^4.1.1"
    },
    "peerDependencies": {
        "contentful": "^7.9.1"
    },
    "devDependencies": {
        "@types/debug": "^4.1.5",
        "@types/jest": "^24.0.17",
        "@types/node": "^14.14.37",
        "contentful": "^7.9.1",
        "dotenv": "^8.0.0",
        "jest": "^24.8.0",
        "prettier": "^1.18.2",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.3"
    },
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "coverage": "jest --coverage",
        "format": "prettier --write 'src/**/*.ts'",
        "prepublishOnly": "npm test",
        "prepack": "rm -rf lib && tsc",
        "version": "npm run format && git add -A src"
    }
}
