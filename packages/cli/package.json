{
    "name": "@contentful-tools/cli",
    "version": "0.7.3",
    "description": "A CLI for generating type declarations based on Contentful content types.",
    "keywords": [
        "contentful",
        "typescript",
        "codegen"
    ],
    "author": "Inside Labs",
    "homepage": "https://github.com/insidelabs/contentful-tools/tree/master/packages/cli#readme",
    "license": "ISC",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "directories": {
        "bin": "bin",
        "lib": "lib",
        "test": "lib/__tests__"
    },
    "files": [
        "/bin",
        "/lib"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/insidelabs/contentful-tools.git",
        "directory": "packages/cli"
    },
    "bugs": {
        "url": "https://github.com/insidelabs/contentful-tools/issues"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "peerDependencies": {
        "@contentful-tools/store": "*"
    },
    "dependencies": {
        "@oclif/command": "^1.5.19",
        "@oclif/config": "^1.13.2",
        "@oclif/plugin-help": "^2.2.0",
        "camelcase": "^5.3.1",
        "contentful": "^7.10.0",
        "contentful-management": "^5.9.0",
        "cosmiconfig": "^6.0.0",
        "debug": "^4.1.1",
        "dotenv": "^8.2.0",
        "fp-ts": "^2.9.2",
        "io-ts": "^2.0.1",
        "listr": "^0.14.3",
        "lodash": "^4.17.15",
        "mkdirp": "^0.5.1",
        "pluralize": "^8.0.0",
        "prettier": "^1.18.2",
        "rimraf": "^3.0.0",
        "rxjs": "^6.5.3"
    },
    "devDependencies": {
        "@contentful-tools/store": "^0.7.3",
        "@types/camelcase": "^5.2.0",
        "@types/debug": "^4.1.5",
        "@types/dotenv": "^8.2.0",
        "@types/listr": "^0.14.2",
        "@types/lodash": "^4.14.144",
        "@types/mkdirp": "^0.5.2",
        "@types/node": "^12.12.5",
        "@types/pluralize": "0.0.29",
        "@types/prettier": "^1.18.3",
        "@types/rimraf": "^2.0.3",
        "jest": "^24.8.0",
        "npm-check": "^5.9.0",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.4.1",
        "typescript": "^3.7.1-rc"
    },
    "oclif": {
        "commands": "./lib/commands",
        "bin": "contentful-tools",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "scripts": {
        "generate": "./bin/contentful-tools generate",
        "sync": "./bin/contentful-tools sync",
        "test": "npm run generate && npm run sync",
        "format": "prettier --write 'src/**/*.ts'",
        "prepack": "rm -rf lib && tsc",
        "version": "npm run format && git add -A src",
        "upgrade": "npm-check -u"
    }
}
